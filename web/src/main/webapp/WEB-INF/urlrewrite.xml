<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE urlrewrite PUBLIC "-//tuckey.org//DTD UrlRewrite 3.0//EN"
    "http://tuckey.org/res/dtds/urlrewrite3.0.dtd">

<urlrewrite default-match-type="wildcard">
    <rule>
        <from>/</from>
        <to type="redirect" last="true">login</to>
    </rule>
    <rule>
        <from>/app/register</from>
        <to>/service/user/mobileSave</to>
    </rule>
    <rule>
        <from>/app/editUser</from>
        <to>/service/user/mobileUpdate</to>
    </rule>
    <rule>
        <from>/app/login</from>
        <to>/j_security_check</to>
    </rule>
    <rule>
        <from>/app/comments</from>
        <to>/service/comments/save</to>
    </rule>
    <rule>
        <from>/app/getUserCredits</from>
        <to>/service/credits/getUserCredits</to>
    </rule>
    <rule>
        <from>/app/listBills</from>
        <to>/service/order/listBills</to>
    </rule>
    <rule>
        <from>/app/createBills</from>
        <to>/service/order/createBills</to>
    </rule>
    <rule>
        <from>/app/cancelBills</from>
        <to>/service/order/cancelBills</to>
    </rule>

    <rule>
        <from>/app/createOrder</from>
        <to>/service/order/createOrder</to>
    </rule>
    <rule>
        <from>/app/getOrders</from>
        <to>/service/order/getOrders</to>
    </rule>
    <rule>
        <from>/app/getBills</from>
        <to>/service/order/getBills</to>
    </rule>
    <rule>
        <from>/app/getApply</from>
        <to>/service/apply/getApply</to>
    </rule>




    <rule>
        <from>/app/getCredits</from>
        <to>/service/order/getCredits</to>
    </rule>


    <rule>
        <from>/app/mobileNotice</from>
        <to>/service/order/createOrder</to>
    </rule>
    <rule>
        <from>/app/getMoney</from>
        <to>/service/money/getMoney</to>
    </rule>
    <rule>
        <from>/app/applyRmb</from>
        <to>/service/apply/applyRmb</to>
    </rule>
    <rule>
        <from>/app/listHistoryEmail</from>
        <to>/service/apply/listAliEmail</to>
    </rule>
    <rule>
        <from>/app/setAp</from>
        <to>/service/aps/setAp</to>
    </rule>
    <rule>
        <from>/app/getAp</from>
        <to>/service/aps/getAp</to>
    </rule>
    <rule>
        <from>/app/reportPoints</from>
        <to>/service/credits/reportPoints</to>
    </rule>
    <rule>
        <from>/app/givePoints</from>
        <to>/service/credits/givePoints</to>
    </rule>



    <rule>
        <from>/alipay/bachnotice</from>
        <to>/service/bachnotice</to>
    </rule>
    <rule>
        <from>/alipay/mobilenotice</from>
        <to>/service/mobilenotice</to>
    </rule>
    <rule>
        <from>/alipay/pay</from>
        <to>/service/apply/bathPay</to>
    </rule>



    <rule>
        <from>/j_security_check</from>
        <to>/j_security_check</to>
    </rule>
    <rule>
        <from>/logout*</from>
        <to>/logout.jsp</to>
    </rule>
    <rule>
        <from>/images/**</from>
        <to>/images/$1</to>
    </rule>
    <rule>
        <from>/scripts/**</from>
        <to>/scripts/$1</to>
    </rule>
    <rule>
        <from>/styles/**</from>
        <to>/styles/$1</to>
    </rule>
    <rule>
        <from>/demo/**</from>
        <to>/demo/$1</to>
    </rule>

    <rule>
        <from>/service/**</from>
        <to>/service/$1</to>
    </rule>
    <rule>
        <from>/jscripts/**</from>
        <to>/jscripts/$1</to>
    </rule>


    <rule>
        <from>/login*</from>
        <to>/login.jsp</to>
    </rule>
    <rule>
        <from>/**</from>
        <to>/service/$1</to>
    </rule>
    <outbound-rule encodefirst="true" match-type="regex">
        <name>Strip URL Session ID's</name>
        <from>^(.*?)(?:\;jsessionid=[^\?#]*)?(\?[^#]*)?(#.*)?$</from>
        <to>$1$2$3</to>
    </outbound-rule>
</urlrewrite>